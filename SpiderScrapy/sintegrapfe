<!----- Envia ----->
<SCRIPT>
var Erro=""
var Op = ""
function Verif(x) {
	if (x == 1) {
		if (document.pfe.cnpj.value == "")  { Erro = "O CNPJ deve ser informado !\n" }
		else {
	  		if (isNaN(document.pfe.cnpj.value)) { Erro += "O CNPJ deve ser numérico !\n" }
			if (document.pfe.cnpj.value.length == 13) { document.pfe.cnpj.value = "0" + document.pfe.cnpj.value }
			else if (document.pfe.cnpj.value.length != 14) { Erro += "O CNPJ deve ter exatamente 14 algarismos !\n" }
		}
		document.pfe.servico.value = "cnpj"
	}
	else if (x == 2) {
		if (document.pfe.ie.value == "")  { Erro = "A Inscrição Estadual deve ser informada !\n" }
		else {
		  if (isNaN(document.pfe.ie.value)) { Erro += "A Inscrição Estadual deve ser numérica !\n" }
		  if (document.pfe.ie.value.length != 12) { Erro += "A Inscrição Estadual deve ter exatamente 12 algarismos !\n" }
		}
		document.pfe.servico.value = "ie"
	}
	Op = x
}
function Envia() {
	if (Erro != "") {
          alert (Erro + "\nCONSULTA NÃO REALIZADA !")
	  Erro = ""
          return false }
	else { 
		document.pfe.hidFlag.value = 1
		return true 
	}
}
function somenteDigitos(campo){
    if((ret=campo.value.search(/[^0-9]/)) > -1)
	   campo.value = campo.value.substr(0,ret);
}



</SCRIPT>
