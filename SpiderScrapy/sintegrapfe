	function autotab(vlCampoAtual,tamanhoMaximo,proxCampo){
		//alert("vlCampoAtual=[" + vlCampoAtual + "]\ntamanhoMaximo=[" + tamanhoMaximo + "]\nlen=[" + vlCampoAtual.length + "]");
		if (vlCampoAtual.length == tamanhoMaximo)
			proxCampo.focus();
	}

function Atualiza()
{
	//alert("document.pfe.hidFlag.value="+document.pfe.hidFlag.value);
	if (document.pfe.hidFlag.value == 1)
	{
		document.pfe.hidFlag.value = 0;
		window.location.reload();
	}
	document.pfe.Key.focus();
}
</script>
